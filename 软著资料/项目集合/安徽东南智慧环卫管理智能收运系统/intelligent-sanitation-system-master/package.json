    "gulp-load-plugins": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "iview": "^3.5.4",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.15",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "net": "^1.0.2",
