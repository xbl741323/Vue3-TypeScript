    "vue-i18n": "^8.12.0",
    "vue-monaco": "^1.2.1",
    "vue-router": "^3.0.7",
    "vue-slider-component": "^3.2.15",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.1.1",
    "vxe-table": "^2.10.10",
    "xe-utils": "^2.8.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-standard": "^4.0.0",
